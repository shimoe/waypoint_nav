<?xml version="1.0" encoding="utf-8"?>
<launch>

  <group ns="gps">
    <!-- NavSat Serial -->
    <node pkg="nmea_navsat_driver" type="nmea_serial_driver" name="nmea_serial_driver" > <!--output="screen"-->
      <!-- for Garmin GPS -->
	    <!--
      <param name="port" value="$(optenv HUSKY_NAVSAT_PORT /dev/ftdi_FTVBIBT1)" />
      <param name="baud" value="$(optenv HUSKY_NAVSAT_BAUD 19200)" />
     	-->
      <!-- For Novatel GPS -->
      <param name="port" value="$(optenv HUSKY_NAVSAT_PORT /dev/ttyACM0)" />
      <param name="baud" value="$(optenv HUSKY_NAVSAT_BAUD 115200)" />
      
      <param name="frame_id" value="GPS_link" />
      <remap from="/gps/fix" to="/navsat/fix" />
 
   </node>

    <!-- NavSat Processing -->
    <node pkg="nmea_navsat_driver" type="nmea_topic_driver" name="nmea_topic_driver">
      <remap from="/gps/fix" to="/navsat/fix" />
    </node>

  </group>

</launch>
